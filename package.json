{
  "name": "strong-globalize",
  "description": "StrongLoop Globalize - CLI and API",
  "version": "3.1.0",
  "keywords": [
    "StrongLoop",
    "globalize",
    "cldr"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/strongloop/strong-globalize.git"
  },
  "author": "Tetsuo Seto <tseto@us.ibm.com>",
  "maintainers": [
    {
      "name": "Tetsuo Seto",
      "email": "setogit@gmail.com"
    }
  ],
  "license": "Artistic-2.0",
  "readmeFilename": "README.md",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "pretest": "eslint ./ && jscs ./",
    "coveralls": "tap --coverage-report=text-lcov | coveralls",
    "test": "tap --bail --coverage --coverage-report=cobertura --timeout=200 test/test-*.*",
    "posttest": "tap --coverage-report=lcov --no-browser && tap --coverage-report=text"
  },
  "nyc": {
    "include": [
      "index.js",
      "browser.js",
      "lib/*.js",
      "bin/*.js"
    ],
    "all": true
  },
  "bugs": {
    "url": "https://github.com/strongloop/strong-globalize/issues"
  },
  "bin": {
    "slt-globalize": "./bin/slt-globalize.js"
  },
  "dependencies": {
    "async": "^2.4.1",
    "debug": "^3.1.0",
    "esprima": "^4.0.0",
    "estraverse": "^4.2.0",
    "g11n-pipeline": "^1.5.1",
    "htmlparser2": "^3.9.2",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "mktmpdir": "^0.1.1",
    "optional": "^0.1.3",
    "os-locale": "^1.4.0",
    "posix-getopt": "^1.2.0",
    "word-count": "^0.2.2",
    "xtend": "^4.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "eslint-config-strongloop": "^2.1.0",
    "intercept-stdout": "^0.1.2",
    "jscs": "^2.11.0",
    "shelljs": "^0.7.8",
    "tap": "^10.7.2"
  }
}
